<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net6.0;net452</TargetFrameworks>
    <RootNamespace>SyntaxSolutions.CsvReader</RootNamespace>
    <AssemblyName>SyntaxSolutions.CsvReader</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Syntax Solutions Ltd</Authors>
    <Description>A simple .Net library used to read comma-separated values (CSV) from a file or data stream. </Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/SyntaxSolutions/csvreader-net</PackageProjectUrl>
    <Product>CsvReader</Product>
    <NeutralLanguage>en</NeutralLanguage>
    <RepositoryUrl>https://github.com/SyntaxSolutions/csvreader-net</RepositoryUrl>
    <Copyright>Syntax Solutions Ltd</Copyright>
    <PackageTags>c#,.Net,CSV</PackageTags>
    <PackageIconUrl></PackageIconUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageReleaseNotes>A simple .Net library used to read comma-separated values (CSV) files</PackageReleaseNotes>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <FileVersion>1.1.0.0</FileVersion>
    <Version>1.1.0</Version>
    <PackageIcon>package-icon.png</PackageIcon>
    <Title>CSV Reader</Title>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\build\package-icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.VisualBasic" Version="10.3.0" />
  </ItemGroup>

</Project>